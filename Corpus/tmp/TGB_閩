king1 - tse3 , sia7 - hue7 thau5 - lang5 sa1 - tioh8 tiong1 - hua5 - bin5 - kok4 kuan5 - lik8 ham7 li7 - ik4 hun1 - phue3 e5 tsham1 - sai3 kuan5 ,
