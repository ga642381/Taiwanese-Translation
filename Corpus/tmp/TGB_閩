mng7 : li2 jin7 - ui5 guan5 - tsu7 - bin5 tsit4 - ma2 tsu2 - kuan1 siong7 - ai3 kuann2 - kin2 sing1 - tshue7 tng2 - lai5 - e5 si7 siann2 ?
tap4 : ti7 tsit4 - pai2 guan5 - tsu7 - bin5 ui2 - uan5 - hue7 ki2 pan7 - e5 se3 - kai3 guan5 - tsu7 - bin5 tian2 lai7 - te2 ,
bo5 - lun7 si7 khi3 seh8 - ke1 iah4 tng2 png7 - tiam3 ,
